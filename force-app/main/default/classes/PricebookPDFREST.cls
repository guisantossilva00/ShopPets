@RestResource(urlMapping='/Document/Representative/Pricebook/*')
global class PricebookPDFREST {
    @HttpGet
    global static String doGet(){
        PageReference pr = Page.PricebookPDF;
        String base64 = EncodingUtil.base64Encode(pr.getContentAsPDF());
        ContentVersion v = new ContentVersion();
        v.versionData = EncodingUtil.base64Decode(base64);
        v.title = 'Tabela-de-preço.pdf';
        v.pathOnClient ='Tabela-de-preço.pdf';
        insert v;
        
        String userId = UserInfo.getUserId();
        Id conDoc = [SELECT ContentDocumentId FROM ContentVersion WHERE Id =: v.Id].ContentDocumentId;
        ContentDocumentLink cDe = new ContentDocumentLink();
        cDe.ContentDocumentId = conDoc;
        cDe.LinkedEntityId = [SELECT id FROM SalesTeam__c WHERE RepresentativeLkp__c =: userId limit 1].Id;
        cDe.Visibility = 'AllUsers';
        insert cDe;  
        
        ContentVersion cv = [select id from contentversion where contentdocumentid =:conDoc];
        
        ContentDistribution distribution = new ContentDistribution();
        distribution.Name = 'Tabela-de-preço';
        distribution.ContentVersionId = cv.id;
        distribution.PreferencesAllowViewInBrowser = true;
        distribution.PreferencesLinkLatestVersion = true;
        distribution.PreferencesNotifyOnVisit = false;
        distribution.PreferencesPasswordRequired = false;
        distribution.PreferencesAllowOriginalDownload = false;
        insert distribution;

        String publicURL = [SELECT DistributionPublicUrl FROM ContentDistribution  WHERE Id =: distribution.Id].DistributionPublicUrl;
        return publicURL;
    }
    
    
